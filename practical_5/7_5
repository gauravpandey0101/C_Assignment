#include<stdio.h>
#include<conio.h>
#include<stdlib.h>
#include<string.h>

int main()
{
 int i,j,c=0;
 char str[10];
 clrscr();
 gets(str);
 for(i=0,j=strlen(str);str[i]!='\0',j>=0;i++,j--)
 {
 c = atoi(strcmp(str[i],str[j]));
 }
 if(c==0)
 {
  printf("Pallindrome");
 }
 else
 {
  printf("Not pallindrome");
 }
 getch();
 return 0;
}
