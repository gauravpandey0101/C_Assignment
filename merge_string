void strcatt()
{
 int i,j;
 char str1[10],str2[10],str3[20];
 printf("Input first string : ");
 gets(str1);
 printf("Input second string : ");
 gets(str2);
 for(i=0,j=strlen(str1);str1[i]!='\0',str2[j]!='\0';i++,j++)
 {
  str3[i] = str1[i];
  str3[j] = str2[i];
 }
 str3[j]='\0';
 puts(str3);
}

