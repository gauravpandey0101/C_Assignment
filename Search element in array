#include<stdio.h>
int main()
{
    int a[5],num,i;
    for(i=0;i<5;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Input Number for Search\n");
    scanf("%d",&num);
    for(i=0;i<5;i++)
    {
        if(a[i] == num)
        {
            printf("%d Number Found at %d Location",num,(i+1));
            break;
        }
    }
    return 0;
}
